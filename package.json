{
	"name": "inikit",
	"version": "2.0.0",
	"description": "Best way to get started with Next.js and React project.",
	"main": "./dist/index.js",
	"scripts": {
		"build": "npx eslint . && npm run clean && tsc && cpx \"templates/**/*\" dist/templates",
		"clean": "rimraf dist",
		"lint": "eslint .",
		"lint:fix": "eslint . --fix",
		"format:check": "prettier --check .",
		"format": "prettier --write .",
		"dev": "tsx index.ts",
		"test": "echo \"Running dummy test\" && exit 0",
		"deploy": "npm run build && npm publish --access public",
		"prepare": "husky"
	},
	"bin": {
		"inikit": "./dist/index.js"
	},
	"files": [
		"dist"
	],
	"lint-staged": {
  "*.{js,ts,tsx}": [
    "eslint --fix",
    "prettier --write"
  ]
}

